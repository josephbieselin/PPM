-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
ELFSIZE += \
io.elf.size \

ELFCHECK += \
io.elf.elfcheck \


# All Target
all: io.elf secondary-outputs

# Tool invocations
io.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MicroBlaze gcc linker'
	mb-gcc -Wl,-T -Wl,../src/lscript.ld -L../../io_bsp/mcs_io/lib -mlittle-endian -mcpu=v8.40.a -mxl-soft-mul -Wl,--no-relax -Wl,--gc-sections -o "io.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

io.elf.size: io.elf
	@echo 'Invoking: MicroBlaze Print Size'
	mb-size io.elf  |tee "io.elf.size"
	@echo 'Finished building: $@'
	@echo ' '

io.elf.elfcheck: io.elf
	@echo 'Invoking: Xilinx ELF Check'
	elfcheck io.elf -hw ../../hw_platform_1/system.xml -pe mcs_io  |tee "io.elf.elfcheck"
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(EXECUTABLES)$(ELFSIZE)$(ELFCHECK)$(S_UPPER_DEPS) io.elf
	-@echo ' '

secondary-outputs: $(ELFSIZE) $(ELFCHECK)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
